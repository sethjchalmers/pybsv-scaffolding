# BSV Teranode Configuration
# Copy this file to .env and fill in your values

# Teranode RPC Configuration
TERANODE_RPC_HOST=localhost
TERANODE_RPC_PORT=9292
TERANODE_RPC_USER=your_rpc_username
TERANODE_RPC_PASSWORD=your_rpc_password

# Asset Server Configuration
TERANODE_ASSET_HOST=localhost
TERANODE_ASSET_PORT=8000

# Network Configuration
# Options: teratestnet, mainnet, testnet
BSV_NETWORK=teratestnet

# Your private key for signing transactions (WIF format)
# WARNING: Never commit real private keys to version control!
BSV_PRIVATE_KEY=your_private_key_here

# Optional: ngrok domain if using ngrok for public access
NGROK_DOMAIN=your_domain.ngrok-free.app
